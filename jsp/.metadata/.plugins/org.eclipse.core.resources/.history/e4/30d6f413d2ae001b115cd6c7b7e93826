<script type="text/javascript">
	$(document).ready(function(){
		// html()/text() - 해당요소의 값을 가져오는 함수
		var h = $('h2').html();
		alert("h : " + h);
		
		var t = $('h2').text();
		alert("t : " + t);
	});

</script>
