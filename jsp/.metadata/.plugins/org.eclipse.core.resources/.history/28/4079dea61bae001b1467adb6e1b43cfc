<script type="text/javascript">
  $(document).ready(function(){
	  // h2 태그 글자색 변경
	  // 속성의 값을 정해주면 값을 지정해주는 것.
	  $('h2').css('color', 'red');
	
	  
	  // 속성명만 쓰면 값을 가져오는 것
	  var c = $('h2').css('color');
	  alert(c);
	  
	  
	  $('h2').css('background', 'black');
  });
</script>
