package practice;

public class Fibonacci {

	public static void main(String[] args) {
		int a1 = 1;
		int a2 = 1;
		int a3 = 0;
		int sum = 0;
		
		System.out.println(a1);
		System.out.println(a2);
		
		while(a3 <= 4000000) {
			
			a3 = a1 + a2;
			a1 = a2;
			a2 = a3; 
			
			if( a3 % 2 == 0) {
				sum += a3;
			}
			System.out.println("a3 = " + a3);
			
		}

		System.out.println("sum = " + sum);
		
		
	}
				
}
