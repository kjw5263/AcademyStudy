<script type="text/javascript">
	$(document).ready(function(){
		// xml.jsp 페이지 이동 후 xml 데이터를 가지고와서 (비동기방식)
		// 해당 페이지에 출력
		//$.ajax("주소",{});  이형태로 써줘도 된다
		$.ajax({
			url:"xml.jsp",
			success:function(data){
				alert(data);
				console.log(data);
			}
		})
	});

</script>
