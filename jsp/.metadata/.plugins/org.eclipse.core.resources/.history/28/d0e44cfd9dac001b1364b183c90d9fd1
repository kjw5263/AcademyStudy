const inner = document.querySelector(".inner");

fetch('https://jsonplaceholder.typicode.com/todos/')
.then(res => {
  // response 처리
  console.log(res);
  // 응답을 JSON 형태로 파싱
  return res.json();
})
.then(data => {
  // json 출력
  console.log(data)
  inner.innerText() = data;
})
.catch(err => {
  // error 처리
  console.log('Fetch Error', err);
});